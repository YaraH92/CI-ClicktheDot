<?xml version="1.0" encoding="utf-8"?>


<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="127dp"
        android:layout_y="168dp"
        android:clickable="true"
        android:focusable="true"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/button_bg_round" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_x="0dp"
        android:layout_y="0dp"
        android:gravity="top|left|start"
        android:orientation="vertical">

        <fragment
            android:id="@+id/fragment21"
            android:name="com.yarahaddad.clickthedot.CurrentTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="216dp"
            android:layout_y="20dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_x="0dp"
        android:layout_y="6dp"
        android:gravity="top|right"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="20dp"
            android:layout_y="21dp"
            android:text="TextView"
            tools:layout_editor_absoluteX="50dp"
            tools:layout_editor_absoluteY="8dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="22dp"
            android:layout_y="53dp"
            android:text="TextView"
            tools:layout_editor_absoluteX="13dp"
            tools:layout_editor_absoluteY="41dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_x="0dp"
        android:layout_y="0dp"
        android:gravity="bottom|left"
        android:orientation="vertical">

        <fragment
            android:id="@+id/fragment5"
            android:name="com.yarahaddad.clickthedot.LocationTracker"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="92dp"
            android:layout_y="353dp" />

        <fragment
            android:id="@+id/fragment6"
            android:name="com.yarahaddad.clickthedot.WifiNetwork"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="38dp"
            android:layout_y="475dp" />

        <fragment
            android:id="@+id/fragment15"
            android:name="com.yarahaddad.clickthedot.WebServer"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_x="207dp"
            android:layout_y="456dp" />
    </LinearLayout>

</AbsoluteLayout>
